                                                    <option value="2">2</option>
                                                    <option value="2.5">2.5</option>
                                                    <option value="3">3</option>
                                                    <option value="3.5">3.5</option>
                                                    <option value="4">4</option>
                                                    <option value="4.5">4.5</option>
                                                    <option value="5">5</option>
                                                    <option value="5.5">5.5</option>
                                                    <option value="6">6</option>
                                                    <option value="6+">6+</option>
                                                </select>
                                            </div>
                                            <div class="col-md-6">
                                                <label class="fieldlabels">Number of Bathroom</label>
                                                <select name="noofbathrooms" class="form-control">
                                                    <option value="">Select Status</option>
                                                    <option value="1">1</option>
                                                    <option value="2">2</option>
                                                    <option value="3">3</option>
                                                    <option value="4">4</option>
                                                    <option value="5">5</option>
                                                    <option value="6">6</option>
                                                    <option value="6+">6+</option>
                                                </select>
                                            </div>
                                            <div class="col-md-6">
                                                <label class="fieldlabels">Number of Parking</label>
                                                <select name="noofbathrooms" class="form-control">
                                                    <option value="">Select Parking</option>
                                                    <option value="1">1</option>
                                                    <option value="2">2</option>
                                                    <option value="3">3</option>
                                                    <option value="4">4</option>
                                                    <option value="5">5</option>
                                                    <option value="6">6</option>
                                                    <option value="6+">6+</option>
                                                </select>
                                            </div>
                                            <div class="col-md-6">
                                                <label class="fieldlabels">View/Facing</label>
                                                <select name="noofbathrooms" class="form-control">
                                                    <option value="">Select View/Facing</option>
                                                    <option value="">Beach View</option>
                                                    <option value="">East</option>
                                                    <option value="">Garden Facing</option>
                                                    <option value="">Golf Course</option>
                                                    <option value="">North</option>
                                                    <option value="">North East</option>
                                                    <option value="">North West</option>
                                                    <option value="">Park Facing</option>
                                                    <option value="">South</option>
                                                    <option value="">South East</option>
                                                    <option value="">South West</option>
                                                    <option value="">West</option>
                                                </select>
                                            </div>
                                            
                                            <div class="col-md-6">
                                                <label class="fieldlabels">Tower/Block</label>
                                                <input type="text" name="towerblock" placeholder="Please enter Block or Tower Name" />
                                            </div>
                                            <div class="col-md-6">
                                                <label class="fieldlabels">Total Floor Count</label>
                                                <input type="text" name="towerblock" placeholder="Please enter Block or Tower Name" />
                                            </div>
                                            <div class="col-md-6">
                                                <label class="fieldlabels">Floor Number</label>
                                                <select name="noofbathrooms" class="form-control">
                                                    <option value="1">1</option>
                                                    <option value="2">2</option>
                                                    <option value="3">3</option>
                                                    <option value="4">4</option>
                                                    <option value="5">5</option>
                                                    <option value="6">6</option>
                                                    <option value="7">7</option>
                                                    <option value="8">8</option>
                                                    <option value="9">9</option>
                                                    <option value="10">10</option>
                                                    <option value="11">11</option>
                                                    <option value="12">12</option>
                                                    <option value="13">13</option>
                                                    <option value="14">14</option>
                                                    <option value="15">15</option>
                                                    <option value="16">16</option>
                                                    <option value="17">17</option>
                                                    <option value="18">18</option>
                                                    <option value="19">19</option>
                                                    <option value="20">20</option>
                                                    <option value="21">21</option>
                                                    <option value="22">22</option>
                                                    <option value="23">23</option>
                                                    <option value="24">24</option>
                                                    <option value="25">25</option>
                                                    <option value="26">26</option>
                                                    <option value="27">27</option>
                                                    <option value="28">28</option>
                                                    <option value="29">29</option>
                                                    <option value="30">30</option>
                                                    <option value="31">31</option>
                                                    <option value="32">32</option>
                                                    <option value="33">33</option>
                                                    <option value="34">34</option>
                                                    <option value="35">35</option>
                                                </select>
                                            </div>
                                            <div class="col-md-6">
                                                <label class="fieldlabels">Unit Number.</label>
                                                <input type="text" name="unitnumber" placeholder="Please enter Unit number" />
                                            </div>
                                        </div>
                                    </div>
                                    <div class="aminities">
                                        <div class="row">
                                            <div class="col-md-12">
                                                <h2 class="fs-title">Amenities Details:</h2>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-md-3 amenities">
                                                <label for="gymnasium"> Gymnasium</label>
                                                <input type="checkbox" id="gymnasium" class="mb-0" name="gymnasium" value="Gymnasium">
                                            </div>
                                            <div class="col-md-3 amenities">
                                                <label for="gymnasium"> Swimming Pool</label>
                                                <input type="checkbox" id="swimmingpool" class="mb-0" name="swimmingpool" value="Swimming Pool">
                                            </div>
                                            <div class="col-md-3 amenities">
                                                <label for="gymnasium"> Badminton Court</label>
                                                <input type="checkbox" id="badmintoncourt" class="mb-0" name="badmintoncourt" value="Badminton Court">
                                            </div>
                                            <div class="col-md-3 amenities">
                                                <label for="gymnasium"> Tennis Court</label>
                                                <input type="checkbox" id="tenniscourt" class="mb-0" name="tenniscourt" value="Tennis Court(s)">
                                            </div>
                                            <div class="col-md-3 amenities">
                                                <label for="gymnasium"> Squash Court</label>
                                                <input type="checkbox" id="squashcourt" class="mb-0" name="squashcourt" value="Squash Court">
